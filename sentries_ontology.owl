<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/sentries#"
     xml:base="http://www.semanticweb.org/sentries"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/sentries"/>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sentries#owned_by">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sentries#Organization"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sentries#Entity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sentries#family_member_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sentries#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sentries#Person"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sentries#leader_of">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sentries#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sentries#Organization"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sentries#operates">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sentries#Entity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sentries#Vessel"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sentries#Aircraft"/>
    </owl:ObjectProperty>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sentries#riskScore">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sentries#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sentries#primaryName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sentries#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="http://www.semanticweb.org/sentries#Entity"/>

    <owl:Class rdf:about="http://www.semanticweb.org/sentries#Person">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sentries#Entity"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/sentries#Organization">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sentries#Entity"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/sentries#Vessel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sentries#Entity"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/sentries#Aircraft">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sentries#Entity"/>
    </owl:Class>
</rdf:RDF>
